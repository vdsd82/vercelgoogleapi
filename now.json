{
    "version": 2,
    "public": false,
    "builds": [
        {
            "src": "app.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "app.py"
        },
        {
            "src": "/googleapi/(.*)",
            "dest": "app.py"
        }
    ]
}